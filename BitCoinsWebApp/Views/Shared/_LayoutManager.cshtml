@using BitCoinsWebApp.Model
@model dynamic

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="@Url.Content("~/Content/themes/manager/plugins/images/favicon.png")" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title - A Finance Company</title>
    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/Content/themes/manager/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet" />
    <!-- Menu CSS -->
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.css")" rel="stylesheet">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/manager/plugins/bower_components/html5-editor/bootstrap-wysihtml5.css")" />
    <!-- toast CSS -->
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/toast-master/css/jquery.toast.css")" rel="stylesheet">
    <!-- morris CSS -->
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/morrisjs/morris.css")" rel="stylesheet">
    <!-- animation CSS -->
    <link href="@Url.Content("~/Content/themes/manager/css/animate.css")" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="@Url.Content("~/Content/themes/manager/css/style.css")" rel="stylesheet">
    <!-- color CSS -->
    <link href="@Url.Content("~/Content/themes/manager/css/colors/green.css")" id="theme" rel="stylesheet">
    <!--alerts CSS -->
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/sweetalert/sweetalert.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/datatables/jquery.dataTables.min.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/manager/plugins/bower_components/dropify/dist/css/dropify.min.css")" type="text/css" >
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/switchery/dist/switchery.min.css")" rel="stylesheet" />
    <!--Owl carousel CSS -->
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/owl.carousel/owl.carousel.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/manager/plugins/bower_components/owl.carousel/owl.theme.default.css")" rel="stylesheet" type="text/css" />
    <!-- jQuery -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/jquery/dist/jquery.min.js")"></script>
</head>
<body>
    @RenderBody()
    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/bootstrap/dist/js/bootstrap.min.js")"></script>
    <!-- Menu Plugin JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.js")"></script>
    <!--slimscroll JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/js/jquery.slimscroll.js")"></script>
    <!--Wave Effects -->
    <script src="@Url.Content("~/Content/themes/manager/js/waves.js")"></script>
    <!--Counter js -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/waypoints/lib/jquery.waypoints.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/counterup/jquery.counterup.min.js")"></script>
    <!--Morris JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/raphael/raphael-min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/morrisjs/morris.js")"></script>
    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/js/custom.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/js/dashboard1.js")"></script>
    <!-- Sparkline chart JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/jquery-sparkline/jquery.sparkline.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/jquery-sparkline/jquery.charts-sparkline.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/toast-master/js/jquery.toast.js")"></script>
    <!--Style Switcher -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/styleswitcher/jQuery.style.switcher.js")"></script>
    <!-- Sweet-Alert  -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/sweetalert/sweetalert.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/sweetalert/jquery.sweet-alert.custom.js")"></script>
    <!-- jQuery file upload -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/dropify/dist/js/dropify.min.js")"></script>
    <!-- wysuhtml5 Plugin JavaScript -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/tinymce/tinymce.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/datatables/jquery.dataTables.min.js")"></script>
    <!-- jQuery file upload -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/dropify/dist/js/dropify.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/switchery/dist/switchery.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/styleswitcher/jQuery.style.switcher.js")"></script>
    <!-- jQuery for carousel -->
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/owl.carousel/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/Content/themes/manager/plugins/bower_components/owl.carousel/owl.custom.js")"></script>
</body>
</html>
